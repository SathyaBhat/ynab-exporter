{
  "extends": "@tsconfig/node16/tsconfig.json",
  "include": ["**/*.ts"],
  "compilerOptions": {
    "target": "es2016",
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
